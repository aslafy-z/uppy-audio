{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "loose": true,
                "targets": {},
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-object-assign",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-typescript",
        "@babel/plugin-syntax-jsx",
        "@babel/plugin-transform-react-jsx"
    ]
}